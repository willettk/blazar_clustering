	Author = {{Pace}, C. and {Salim}, S.},
	Date-Added = {2015-02-18 00:08:43 +0000},
	Date-Modified = {2015-02-18 00:08:43 +0000},
	Eprint = {1403.0003},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrophysics, Astrophysics - Galaxy Astrophysics},
	Month = feb,
	Primaryclass = {astro-ph.CO},
	Title = {{Satellites of Radio AGN in SDSS: Insights into AGN Triggering and Feedback}},
	Year = 2014,
