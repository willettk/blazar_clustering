	Keywords = {Galaxies: BL Lacertae Objects: General, Galaxies: Active, Galaxies: Quasars: General, Radiation Mechanisms: Nonthermal, Radio Continuum: Galaxies, X-Rays: Galaxies},
	Month = may,
	Pages = {128-142},
	Title = {{What Types of Jets Does Nature Make? A New Population of Radio Quasars}},
