	Date-Modified = {2015-02-18 00:08:17 +0000},
	Doi = {10.1111/j.1365-2966.2009.15197.x},
	Eprint = {0806.4930},
	Journal = {\mnras},
	Keywords = {catalogues , surveys , galaxies: luminosity function, mass function , large-scale structure of Universe},
