	Date-Modified = {2015-02-18 00:10:32 +0000},
	Doi = {10.1086/512477},
	Eprint = {arXiv:astro-ph/0701239},
	Journal = {\apj},
	Keywords = {Galaxies: Active, Galaxies: Clusters: General, Galaxies: Quasars: General},
	Month = apr,
	Pages = {1096-1105},
	Title = {{The Environment of Local Ultraluminous Infrared Galaxies}},
